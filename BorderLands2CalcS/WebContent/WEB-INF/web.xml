<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BorderLands2CalcS</display-name>
  
  <servlet>
  	<description>A servlet that handles web requests and links them to the appropriate web service via a handler.</description>
  	<servlet-name>BorderLands2CalcServlet</servlet-name>
  	<servlet-class>cmd.servlet.borderLands2CalcServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
